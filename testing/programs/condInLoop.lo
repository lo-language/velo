main is <-> (log) {

    num = 0;
    val = 0;

    while num < 3 {

        if num > 1 {
            @log <- "hello hello!\n";
        }
        else {
            @log <- "howdy!\n";
        }

        num++;
    }

    while num < 5 {

        @log <- "ok.\n";
        num++;
    }

    reply num;
};