/*
 * iterative fibonacci number calculator
 */

main is <-> (n) {

    if n < 0 {
        fail "Whatsamatta, you?";
    }

    if n <= 1 {
        reply n;
    }

    j = 0;
    k = 1;
    num = 1;
    count = 2;

    while count <= n {

        num = j + k;
        j = k;
        k = num;

        count++;
    }

    reply num;
};