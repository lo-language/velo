/**
 *                       E.T.
 *                  the Exa Tester
 */

receive args, io, env;

// the first module is "classy"; the second a simple procedure
createTest  = acquire("Test");
printReport = acquire("PrintReport");
testModule  = acquire(args[1]);

// create the test
test = createTest(testModule);
report = test.run();

// print the test report
printReport(report, io.out);
