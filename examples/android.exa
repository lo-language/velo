receive args, io;

numPoints is 2;

// recursively iterate
// label points as 0-8

explore = >>:

    receive path;

    io.stdout.write ~ "exploring from `path`\n";

    point = 0;

    while point < numPoints:

        if point in path:
            io.stdout.write ~ "`point` exists in `path`\n";
        else:
            point -> path;
            explore(path);

        point++;
;

found = explore([]);

io.stdout.write("`found`\n");