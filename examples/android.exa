receive args, io;

numPoints is 2;

// recursively iterate
// label points as 0-8

explore = >>:

    receive path;

    point = 0;

    while point < numPoints:

        if point in path:
            io.stdout.write("bummer\n");
        else:
            path <+ point;
            explore(path);

        point++;

    io.stdout.write("`path`\n");
    reply 45;
;

found = explore([]);