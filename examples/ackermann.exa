/**
 * Hilariously naïve computation of the Ackermann–Péter function
 */

receive args, io;

// todo convert these args from strings to numbers

m = args[0];
n = args[1];

if m == 0:
    reply n + 1;

if m > 0 and n == 0:
    reply recur(m - 1, 1);

if m > 0 and n > 0:
    reply recur(m - 1, recur(m, n - 1));