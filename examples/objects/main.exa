/*
 * usage: exa main.exa <side length>
 */

references:

Square  <http://x-language.org/dev/libs/Square>

main is -> (args, io, system) {

    squareSide = args[0];

    square = Square:create(squareSide);

    io.out.write("side length is `square.getSide()`\n");

    area = square.getArea();

    io.out.write(square.toString());
    io.out.write("\n`area`\n");
};

