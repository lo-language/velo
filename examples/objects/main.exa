/*
 * usage: exa main.exa <side length>
 */

makeSquare is <Square>;

receive args, io, system;

squareSide = args[0];

square = makeSquare(squareSide);

io.stdout.write("side length is `square.getSide()`\n");

area = square.getArea();

io.stdout.write(square.toString());
io.stdout.write("\n`area`\n");
