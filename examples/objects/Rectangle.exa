receive width, height, name;

reply {
    getHeight: >>:
        reply height;
    ,
    getWidth: >>:
        reply width;
    ,
    getArea: >>:
        reply height * width;
    ,
    setHeight: >>:
        receive height;
    ,
    setWidth: >>:
        receive width;
    ,
    toString: >>:
        reply "`name`: `width` x `height`";
};